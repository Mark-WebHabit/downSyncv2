{
  "expo": {
    "name": "KDs App",
    "slug": "DownSync",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/logos/logo_splash.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.markdev03.DownSync"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logos/logo.png",
        "backgroundColor": "#ffffff",
        "permissions": ["SYSTEM_ALERT_WINDOW"]
      },
      "package": "com.markdev03.DownSync"
    },
    "web": {
      "favicon": "./assets/logos/logo.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/logos/logo_splash_big.png",
          "dark": {
            "image": "./assets/logos/logo_splash_big.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 400
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d9d42d2d-0e47-4455-bdab-4eb03d3c45eb"
      }
    }
  }
}
