{
  "expo": {
    "name": "DownSync",
    "slug": "DownSync",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/logos/logo_splash_smaller.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logos/logo_splash_smaller.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.markdev03.DownSync"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logos/logo_splash_smaller.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.markdev03.DownSync"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ],
    "extra": {}
  }
}
